{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "include": [
    "src/client",
  ],
  "compilerOptions": {
    "rootDir": "src/client",
    "noEmit": true,
    "lib": [
      "ESNext",
    ],
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "allowImportingTsExtensions": true,
  },
}
